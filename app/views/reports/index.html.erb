<p id="notice"><%= notice %></p>

<h1>Reports</h1>

<table>
  <thead>
    <tr>
      <th>Generate by</th>
      <th>Begin date</th>
      <th>End date</th>
      <th>F unit</th>
      <th>F state</th>
      <th>F kind</th>
      <th>F onu</th>
      <th>F blend</th>
      <th>F code</th>
      <th>F total</th>
      <th>Collection</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <td><%= report.generate_by %></td>
        <td><%= report.begin_date %></td>
        <td><%= report.end_date %></td>
        <td><%= report.f_unit %></td>
        <td><%= report.f_state %></td>
        <td><%= report.f_kind %></td>
        <td><%= report.f_onu %></td>
        <td><%= report.f_blend %></td>
        <td><%= report.f_code %></td>
        <td><%= report.f_total %></td>
        <td><%= report.collection %></td>
        <td><%= link_to 'Show', report %></td>
        <td><%= link_to 'Edit', edit_report_path(report) %></td>
        <td><%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Report', new_report_path %>
